{
  "name": "bowling-api-ts",
  "version": "0.0.2",
  "description": "Bowling API and Console App",
  "main": "dist/index.js",
  "types": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "build:netlify": "npm run prebuild && tsc -p tsconfig.netlify.json && copyfiles \"package.json\" dist/functions/api && cd dist/functions/api && npm install --only=prod",
    "test": "jest --collectCoverage",
    "test:cli": "jest --collectCoverage --group=cli",
    "test:api": "jest --collectCoverage --group=api",
    "test:bdd": "cucumber-js -p default",
    "start": "ts-node src/index.ts",
    "start:cli": "ts-node src/cli/index.ts",
    "watch-node": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/unionhillssoftware/bowling-api-ts.git"
  },
  "author": "Marc Hamilton",
  "license": "ISC",
  "homepage": "https://bitbucket.org/unionhillssoftware/bowling-api-ts#readme",
  "devDependencies": {
    "@cucumber/cucumber": "^7.2.1",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.2",
    "@types/prompts": "^2.0.13",
    "@types/swagger-ui-express": "^4.1.2",
    "copyfiles": "^2.4.1",
    "cucumber-tsflow": "^4.0.0-rc.1",
    "jest": "^27.3.1",
    "jest-express": "^1.12.0",
    "jest-runner-groups": "^2.1.0",
    "netlify-lambda": "^2.0.15",
    "nodemon": "^2.0.14",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typed-rest-client": "^1.8.6",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@netlify/functions": "^0.7.2",
    "@overnightjs/core": "^1.7.6",
    "cli-table3": "^0.6.0",
    "colors": "^1.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-list-routes": "^1.1.3",
    "http-status-codes": "^2.1.4",
    "jet-logger": "^1.0.6",
    "lodash": "^4.17.21",
    "prompts": "^2.4.1",
    "serverless-http": "^2.7.0",
    "swagger-ui-express": "^4.1.6"
  }
}
